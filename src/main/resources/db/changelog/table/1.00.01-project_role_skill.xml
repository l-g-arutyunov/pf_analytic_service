<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="1.00.01-project_role_skill-0001" author="sthoomy" failOnError="true">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="project_role_skill"/>
            </not>
        </preConditions>
        <createTable tableName="project_role_skill" schemaName="pf">
            <column name="skill_id" type="bigint">
                <constraints foreignKeyName="fk_project_role_skill_skill_id" referencedTableName="skill" referencedTableSchemaName="pf" referencedColumnNames="id"
                             nullable="false"/>
            </column>
            <column name="project_role_id" type="bigint">
                <constraints foreignKeyName="fk_project_role_skill_project_role_id" referencedTableName="project_role" referencedTableSchemaName="pf" referencedColumnNames="id"
                             nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>